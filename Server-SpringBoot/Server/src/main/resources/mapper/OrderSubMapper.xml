<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="fun.cyhgraph.mapper.OrderSubMapper">

    <resultMap id="OrderSubResultMap" type="fun.cyhgraph.entity.OrderSub">
        <id property="id" column="id"/>
        <result property="orderId" column="order_id"/>
        <result property="windowId" column="window_id"/>
        <result property="windowName" column="window_name"/>
        <result property="status" column="status"/>
        <result property="amount" column="amount"/>
        <result property="estimatedTime" column="estimated_time"/>
        <result property="startTime" column="start_time"/>
        <result property="completeTime" column="complete_time"/>
        <result property="pickupTime" column="pickup_time"/>
    </resultMap>

    <insert id="insertBatch" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO order_sub (order_id, window_id, window_name, status, amount, estimated_time) VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.orderId}, #{item.windowId}, #{item.windowName}, #{item.status}, #{item.amount}, #{item.estimatedTime})
        </foreach>
    </insert>

</mapper>
